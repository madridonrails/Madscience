<%= object_messages %>
<% unless @unavailabilities.empty? %>
  <h1><%= t 'unavailabilities.unavailabilities' %></h1>
  <%= render :partial => 'filter' %>
  <table id="items-list">
    <%= render :partial => 'header' %>
    <%= render :partial => 'unavailability', :collection => @unavailabilities %>
  </table>
<% end %>
  <div id="index-add-button">
    <%=  link_to image_tag('plus_32.png', :class => 'add-button', :alt => t('general.new'), :title => t('general.new')), new_unavailability_url %>
  </div>
<%= will_paginate(@unavailabilities, :prev_label => t('pagination.prev'), :next_label => t('pagination.next'), :params => params.clone.delete_if{|k,v| k == 'controller' || k == 'action' || k == 'page'}) %>
